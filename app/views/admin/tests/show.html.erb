<h2><%= @test.title %></h2>
<h3>Category: <%= @test.category.title %>
<h3>Level: <%= @test.level %></h3>

<%= link_to 'Create new question for this test', new_admin_test_question_url(@test) %>

<table>
  <thead>
    <th>Question</th>
    <th colspan="3">Actions</th>
  </thead>

  <tbody>
    <%= render @test.questions %>
  </tbody>

</table>

<%= link_to 'Edit', edit_admin_test_path(@test) %>
<%= link_to 'Back', admin_tests_path %>